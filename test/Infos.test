#!/usr/bin/env tclkit8.6
#
lappend auto_path ./lib ../lib

package require acorn
source  acorn-model.tcl

acorn::model Zern {
    surface A	{ R    0.0	K  0.0	n  1.00	thickness 	 200 }
    surface B	{ R -400.0	K -1.0	n -1.00	thickness	-200 type zernike p3 10 p6 .0001 }
    surface C	{ R    0.0	K  0.0	n  1.00	thickness	   0 }
}

puts [acorn::info 1 [Zern B get info]]

exit
acorn::mkrays rays nx 25 ny 25 box 25

#puts [Zern B get p0]
#puts [Zern B get p1 p2 p3 p4 p5 p6 p7 p8 p9 p10]

Zern trace rays

acorn::prays rays
