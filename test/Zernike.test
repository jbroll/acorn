#!/usr/bin/env tclkit8.6
#
lappend auto_path ./lib ../lib

package require acorn

acorn::model Zern {
    surface A	{ R    0.0	K  0.0	n  1.00	thickness 	 200 }
    surface B	{ R -400.0	K -1.0	n -1.00	thickness	-200 type zernike nradius 25 nzterms 11 z4 -0.1 z5 -0. }
    surface C	{ R    0.0	K  0.0	n  1.00	thickness	   0 }
}


acorn::mkrays rays nx 25 ny 25 box 25 circle 1
#acorn::mkrays rays nx 3 ny 3 box 25

puts [Zern B get z1]

Zern trace rays

acorn::prays rays
